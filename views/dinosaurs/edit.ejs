<h2>Update Your Dino Info</h2>

<form method="POST" action="/dinosaurs/<%= dinosaur.id %>/?_method=PUT" >
  <label for="type">Type</label>
  <input type="text" name="type" value="<%= dinosaur.type %>" />

  <label for="name">Name</label>
  <input type="text" name="name" value="<%= dinosaur.name %>" />

  <input type="submit" value="UPDATE MY DINO" />

</form>